{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build:dev": {
      "dependsOn": ["^build:dev"]
    },

    "build:prod": {
      "dependsOn": ["^build:prod"]
    },

    "clean-build": {
      "dependsOn": ["^clean-build"]
    },

    "publish": {
      "dependsOn": ["build:prod"]
    }
  }
}
