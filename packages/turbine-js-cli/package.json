{
  "name": "@meroxa/turbine-js-cli",
  "description": "The internal Javascript CLI for Turbine",
  "version": "1.3.7",
  "main": "lib/index.js",
  "author": "James Martinez <james@meroxa.io>",
  "license": "SEE LICENSE IN LICENSE.MD",
  "files": [
    "lib/**/*",
    "templates/**/*"
  ],
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.14.2",
    "@types/qunit": "^2.19.3",
    "@types/sinon": "^10.0.11",
    "@types/tar-fs": "^2.0.1",
    "@types/targz": "^1.0.1",
    "nyc": "^15.1.0",
    "qunit": "^2.19.3",
    "sinon": "^14.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@meroxa/meroxa-js": "^1.7.1",
    "@meroxa/turbine-js-framework": "^1.1.2",
    "fs-extra": "^10.0.0",
    "tar-fs": "^2.1.1",
    "targz": "^1.0.1",
    "ts-results": "^3.3.0",
    "tslib": "^2.5.0"
  },
  "bin": "bin/turbine-js-cli",
  "scripts": {
    "build:dev": "npm run clean-build && tsc",
    "build:prod": "npm run clean-build && tsc --sourceMap false",
    "clean-build": "rm -rf lib",
    "test": "qunit --require ./test/setup.js test/**/*.ts",
    "test:coverage": "nyc --no-clean --temp-dir ../../.nyc_output npm run test && npm run test:coverage-report:json",
    "test:coverage-report:json": "nyc report --reporter json-summary --temp-dir ../../.nyc_output --report-dir ../../nyc-coverage-report --exclude-after-remap false",
    "test:coverage-report": "nyc report --reporter text --temp-dir ../../.nyc_output --report-dir ../../nyc-coverage-report --exclude-after-remap false"
  }
}
